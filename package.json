{
  "name": "@magiclabs/wagmi-connector",
  "version": "1.0.2",
  "description": "wagmi connector to connect with Magic SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": "https://github.com/EveripediaNetwork/wagmi-magic-connector",
  "license": "MIT",
  "keywords": [
    "wagmi",
    "extension",
    "magic"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-esm-fix --target='dist' --ext='.js'",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "yarn lint --apply",
    "watch:build": "tsc -p tsconfig.json -w",
    "release": "yarn build && changeset publish"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.0",
    "@magic-ext/connect": "^9.1.0",
    "@magic-ext/oauth": "^11.1.3",
    "@magic-sdk/provider": "^17.1.2",
    "@wagmi/core": "^1.0.1",
    "magic-sdk": "^17.1.3",
    "tsc-esm-fix": "^2.20.10"
  },
  "devDependencies": {
    "changeset": "^0.2.6",
    "rome": "12.0.0",
    "typescript": "^4.7.4",
    "viem": "^0.3.18"
  },
  "peerDependencies": {
    "viem": "^0.3.18"
  },
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
